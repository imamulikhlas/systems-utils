{
    "name": "alexafers/system-utils",
    "description": "System utility tools for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AlexaFers",
            "email": "alexa@fers.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AlexaFers\\SystemUtils\\": "src/"
        },
        "files": [
            "src/bootstrap.php",
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexaFers\\SystemUtils\\SystemUtilsServiceProvider"
            ],
            "aliases": {
                "SystemUtils": "AlexaFers\\SystemUtils\\Facades\\SystemUtils"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}